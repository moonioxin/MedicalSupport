<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD SQL Map Config 3.0//EN"  
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.medicalsupport.entity.Admin" alias="Admin"/>
		<typeAlias type="com.medicalsupport.httpentity.AdminReqParam" alias="AdminReqParam"/>
		<typeAlias type="com.medicalsupport.entity.Member" alias="Member"/>
		<typeAlias type="com.medicalsupport.httpentity.MemberReqParam" alias="MemberReqParam"/>
		<typeAlias type="com.medicalsupport.entity.PointInfo" alias="PointInfo"/>
		<typeAlias type="com.medicalsupport.entity.PointExchange" alias="PointExchange"/>
		<typeAlias type="com.medicalsupport.httpentity.PointExchangeReqParam" alias="PointExchangeReqParam"/>   
		<typeAlias type="com.medicalsupport.entity.Product" alias="Product"/>
		<typeAlias type="com.medicalsupport.httpentity.ProductReqParam" alias="ProductReqParam"/>
		<typeAlias type="com.medicalsupport.entity.Department" alias="Department"/>
		<typeAlias type="com.medicalsupport.httpentity.DepartmentReqParam" alias="DepartmentReqParam"/>
		<typeAlias type="com.medicalsupport.entity.RechargeRecord" alias="RechargeRecord"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/Admin.xml"/>
		<mapper resource="mybatis/Member.xml"/>
		<mapper resource="mybatis/PointInfoMapper.xml"/>
		<mapper resource="mybatis/PointExchangeMapper.xml"/>
		<mapper resource="mybatis/ProductMapper.xml" />
		<mapper resource="mybatis/DepartmentMapper.xml" />
		<mapper resource="mybatis/RechargeRecordMapper.xml" />
	</mappers>
	
</configuration>